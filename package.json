{
  "name": "n8n-nodes-kipflow",
  "version": "1.0.1",
  "description": "n8n node for Kipflow CNPJ enrichment",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "kipflow",
    "cnpj",
    "brazil",
    "enrichment"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Kipflow/n8n-kipflow-node",
  "author": {
    "name": "Kipflow",
    "email": "contato@kipflow.io"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kipflow/n8n-kipflow-node.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepublishOnly": "npm run build && npm run lint",
    "release:patch": "npm version patch && git push && git push --tags",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:major": "npm version major && git push && git push --tags"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/KipflowApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/KipflowCnpjEnrichment/KipflowCnpjEnrichment.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.0.0",
    "typescript": "~5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=18.10",
    "npm": ">=9.0.0"
  }
}
